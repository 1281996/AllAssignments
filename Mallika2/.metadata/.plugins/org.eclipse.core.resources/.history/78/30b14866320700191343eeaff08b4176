
import java.util.ArrayList; 
class Demo { 
public void show() 
    { 
        ArrayList<Integer> list = new ArrayList<Integer>(); 
        list.add(4); 
        list.add(7); 
        list.add(1); 
        for (Integer number : list) { 
            System.out.print(number + " "); 
        } 
    } 
} 
